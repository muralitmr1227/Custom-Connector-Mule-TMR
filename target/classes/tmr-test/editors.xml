<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" url="http://www.mulesoft.org/schema/mule/tmr-test" prefix="tmr-test">
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="TMRTest" description="" icon="" localId="parentConfig" image="" abstract="true" extends="http://www.mulesoft.org/schema/mule/tmr-test/abstractTmr-testConnectorGeneric"></global-cloud-connector>
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="TMRTest: Configuration" description="Global Configuration configuration information." icon="icons/theme.classic/tmr-test-connector-small.png" localId="config" image="icons/theme.classic/tmr-test-connector-large.png" abstract="false" extends="http://www.mulesoft.org/schema/mule/tmr-test/parentConfig" connectivityTesting="off" metaData="static" metaDataKeyDisplay="DISPLAY_NAME" supportsUserDefinedMetaData="false">
